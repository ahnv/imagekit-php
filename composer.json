{
    "name": "imagekit/imagekit",
    "type": "library",
    "description": "PHP library for Imagekit",
    "keywords": [
        "library"
    ],
    "license": "MIT",
    "require": {
        "fzaninotto/faker": "^1.9",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0",
        "ext-json": "*",
        "ext-gmp": "*",
        "guzzlehttp/streams": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "ImageKit\\": "src/ImageKit/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ImageKit\\Tests\\": "tests/"
        }
    }
}
